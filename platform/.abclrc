
;; Changes larkc's homedir to ~/site-lisp/
;; so this only loads once as it changed the homedir
(when (probe-file "~/site-lisp/larkc-home.lisp")
   (load "~/site-lisp/larkc-home.lisp"))

#-quicklisp
(let ((quicklisp-init (merge-pathnames "site-lisp/quicklisp/setup.lisp" (directory-namestring *load-truename*))))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)))
    
#+quicklisp
(pushnew (merge-pathnames "site-lisp/" (directory-namestring *load-truename*)) ql:*local-project-directories*)



