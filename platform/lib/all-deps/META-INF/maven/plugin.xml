        </parameter>
        <parameter>
          <name>validateCMD</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
      </parameters>
      <configuration>
        <cmdPath implementation="java.lang.String" default-value="src/main/resources/CycModelDescription.xml">${mapi.cmdPath}</cmdPath>
