        <forceRegenerate implementation="boolean" default-value="false">${mapi.forceRegenerate}</forceRegenerate>
        <generateUI implementation="boolean" default-value="false">${mapi.generateUI}</generateUI>
        <generateWS implementation="boolean" default-value="false">${mapi.generateWS}</generateWS>
        <srcPath implementation="java.lang.String" default-value="target/generated-sources/mapi">${mapi.srcPath}</srcPath>
        <validateCMD implementation="boolean" default-value="true">${mapi.validateCMD}</validateCMD>
      </configuration>
