
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.cyc.parentPom</groupId>
        <artifactId>parentPom</artifactId>
        <version>0.0.1-SNAPSHOT</version>
        <relativePath>../../com.cyc.parentPom</relativePath>
    </parent>
    <name>Query API</name>
    <groupId>com.cyc</groupId>
    <artifactId>query</artifactId>
    <packaging>jar</packaging>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>2.9</version>
                <configuration>
                    <detectLinks>true</detectLinks>
                </configuration>
            </plugin>
        </plugins>
    </build>
  
    <url>http://maven.apache.org</url>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <slf4jVersion>1.7.5</slf4jVersion>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <version>${slf4jVersion}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-log4j12</artifactId>
            <version>${slf4jVersion}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.16</version>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>com.cyc.cat.common</groupId>
            <artifactId>cycAppToolkitCommon</artifactId>
        </dependency>
        <dependency>
            <groupId>com.cyc.library</groupId>
            <artifactId>nlAPI</artifactId>
        </dependency>
        <dependency>
            <groupId>org.opencyc</groupId>
            <artifactId>cyclQuery</artifactId>
        </dependency>
        <dependency> 
            <groupId>org.opencyc</groupId>
            <artifactId>formulaTemplate</artifactId>
        </dependency>
        <dependency>
            <groupId>com.cyc.library</groupId>
            <artifactId>KBAPI</artifactId>
        </dependency>
        <dependency>
            <groupId>org.opencyc</groupId>
            <artifactId>proofView</artifactId>
        </dependency>
        <dependency>
            <groupId>com.cyc.library.methodcaching</groupId>
            <artifactId>methodCaching</artifactId>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>net.htmlparser.jericho</groupId>
            <artifactId>jericho-html</artifactId>
            <version>3.3</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>com.cyc.library</groupId>
            <artifactId>queryResultSet</artifactId>
        </dependency>
        <!--
        <dependency>
            <groupId>com.cyc.nl</groupId>
            <artifactId>scgWSClient</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        -->
    </dependencies>
</project>
