  xsi:schemaLocation="http://ws.opencyc.org/xsd/EBMT http://ws.opencyc.org/xsd/EBMT.xml"
  xmlns="http://ws.opencyc.org/xsd/EBMT">  
  
  <template>
    <examples>
      <example>itself</example>
    </examples>
    <nlPattern class="2000001">itself</nlPattern>
    <anaphoricReference>$NonHumanAnimal#1</anaphoricReference>
    <cyclPattern>
           (#$equalSymbols ?X $NonHumanAnimal#1)
    </cyclPattern>
    <type>$NonHumanAnimal#1</type>
    <variable>?X</variable>
  </template>      

