public class DialogFsmAction {

    protected String action;

    /**
     * Constructs a new DialogFsmAction object givent the action.
     *
     * @@param action the action
     */
    protected DialogFsmAction(String action) {
        this.action = action;
    }

    /**
     * Returns the action.
     *
     * @@return the action
     */
    public String getAction () {
        return action;
    }

}@


