@prefix larkc: <http://larkc.eu/plugin#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

larkc:XMLDocument	rdf:type rdfs:Class ;
			rdfs:subClassOf larkc:InformationSet .
			
larkc:XML2RDFTransformerTransformer
	rdf:type            rdfs:Class;
	rdfs:subClassOf     larkc:InformationSetTransformer;
	larkc:hasInputType  larkc:XMLDocument;
	larkc:hasOutputType larkc:RdfGraph.	